/**Faça um algoriti */
 programa {
  funcao inicio() {
    inteiro numero
    //solicite ao usuario que digite um número
    escreva("digite um número inteiro: ")
    leia(numero)
    se(numero % 2==0){
      escreva("O numero ",numero , " é par")
    }senao{
      escreva("o número ",numero , " é impar")
    }
  }
}
